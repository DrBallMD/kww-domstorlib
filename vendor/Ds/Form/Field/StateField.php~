<?php
namespace Estate\Search\Form\Field;

/**
 * Description of StateField
 *
 * @author pahhan
 */
class StateField extends \Spv_Form_Field_Select
{
    public function __construct()
    {
        parent::__construct('state', array(
            'templating_key' => 'twig_form',
            'template' => 'fields/state.html.twig',
            'multiple' => TRUE,
            'label' => 'Состояние:',
            'required' => FALSE,
        ));
    }
}

