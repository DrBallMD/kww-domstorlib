<div class="domstor_delivery">
    <h3>Сдача</h3>
    {% if block.delivered %}
        <p>Объект сдан</p>
    {% else %}
        {% if block.deliveryQuarter and block.deliveryYear %}
            <p>{{ block.deliveryQuarter }} - {{ block.deliveryYear }}</p>
        {% elseif block.deliveryQuarter %}
            <p>{{ block.deliveryQuarter }}</p>
        {% elseif block.deliveryYear %}
            <p>{{ block.deliveryYear }}</p>
        {% endif %}            
    {% endif %}
</div>
