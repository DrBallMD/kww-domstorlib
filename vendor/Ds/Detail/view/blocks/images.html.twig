<div class="domstor_images">
    {% if block.photos is not empty %}
    <div class="domstor_photo">
        {% for photo in block.photos %}
            <a href="{{ photo }}" class="modal" rel="1"><img src="{{ photo }}" alt="Фото{{ loop.index }}"/></a>
        {% endfor %}
    </div>
    {% endif %}

    {% if block.plans is not empty %}
    <div class="domstor_plan">
        {% for plan in block.plans %}
            <a href="{{ plan }}" class="modal" rel="2"><img src="{{ plan }}" alt="План{{ loop.index }}"/></a>
        {% endfor %}
    </div>
    {% endif %}

    {% if block.flash is not empty %}
    <div class="domstor_flash">
        <a href="{{ block.flash }}" title="Посмотреть ролик" class="domstor_flash_link"><img src="http://domstor.ru/img/play.png" alt="Play" width="80" /></a>
    </div>
    {% endif %}
</div>
